#!/usr/bin/env ruby

require "conway_gol"

world = ConwayGameOfLife::World.new

# acorn-style seed data
[
  [22, 21],
  [21, 23],
  [22, 23],
  [24, 22],
  [25, 23],
  [26, 23],
  [27, 23]
].each do |(x, y)|
  world.spawn(x, y)
end

visualizer = ConwayGameOfLife::Visualizer.new(world)
visualizer.show
